{% set anchorId = "module-screenshot" %}
<h2 id="{{ anchorId }}">{% include "../../includes/anchor-link.njk" %} screenshot</h2>

{% for image in images %}

  <h3 id="screenshot-{{ image.breakpoint }}">{{ image.breakpoint }}</h3>
  <a href="{{ image.src }}" target="_blank" rel="noopener" class="inline-block">
    <img src="{{ image.src }}" alt="Screenshot for {{ image.breakpoint }}" class="block" style="max-height: 200px;">
  </a>

{% endfor %}
