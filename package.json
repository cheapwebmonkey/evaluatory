{
  "name": "evaluatory",
  "description": "Web page evaluation with a focus on accessibility",
  "version": "1.6.0",
  "author": "Darek Kay <hello@darekkay.com> (https://darekkay.com/)",
  "bin": {
    "evaluatory": "bin/evaluatory.js"
  },
  "bugs": {
    "url": "https://github.com/darekkay/evaluatory/issues"
  },
  "dependencies": {
    "@darekkay/logger": "1.1.1",
    "axe-core": "^4.1.4",
    "axios": "0.21.0",
    "dayjs": "1.10.4",
    "fast-xml-parser": "3.19.0",
    "fs-extra": "9.1.0",
    "highlight.js": "10.7.2",
    "html-validate": "4.10.0",
    "json5": "2.2.0",
    "kleur": "4.1.4",
    "lodash": "4.17.21",
    "nunjucks": "3.2.3",
    "open": "8.0.6",
    "playwright": "1.10.0",
    "sywac": "1.3.0",
    "tiny-async-pool": "1.2.0"
  },
  "devDependencies": {
    "@darekkay/eslint-config": "2.4.0",
    "@darekkay/gulp": "1.6.3",
    "@darekkay/styles": "2.3.5",
    "cross-env": "7.0.3",
    "eslint": "7.17.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-unicorn": "26.0.1",
    "http-server": "0.12.3",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin",
    "src"
  ],
  "homepage": "https://darekkay.com/evaluatory/",
  "keywords": [
    "a11y",
    "accessibility",
    "analyze",
    "audit",
    "axe-core",
    "evaluate",
    "inspect",
    "lint",
    "monitor",
    "validator",
    "playwright"
  ],
  "license": "MIT",
  "main": "bin/evaluatory.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/evaluatory.git"
  },
  "scripts": {
    "build": "gulp build",
    "ci": "cross-env CI=true npm run lint && npm run test",
    "clean": "rimraf docs/",
    "dev": "node bin/evaluatory.js --config config.example.json5",
    "dev:debug": "node bin/evaluatory.js --config config.example.json5 --verbose",
    "dev:server": "hs -p 9005 mock/",
    "docs": "node bin/evaluatory.js --config config.demo.json5 --no-open-results --output docs/demo",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "npm run ci",
    "test": "jest"
  }
}
